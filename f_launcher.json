{
  "title": "F-Launcher",
  "rules": [
    {
      "description": "F-Launcher",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "d",
            "modifiers": {
              "mandatory": [

              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open -a finder.app ~/Dropbox"
            }
          ],
          "conditions": [
            {
              "type": "variable_if",
              "name": "launcher_mode",
              "value": 1
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "simultaneous": [
              {
                "key_code": "f"
              },
              {
                "key_code": "d"
              }
            ],
            "simultaneous_options": {
              "key_down_order": "strict",
              "key_up_order": "strict_inverse",
              "to_after_key_up": [
                {
                  "set_variable": {
                    "name": "launcher_mode",
                    "value": 0
                  }
                }
              ]
            },
            "modifiers": {
              "mandatory": [

              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "set_variable": {
                "name": "launcher_mode",
                "value": 1
              }
            },
            {
              "shell_command": "open -a finder.app ~/Dropbox"
            }
          ],
          "parameters": {
            "basic.simultaneous_threshold_milliseconds": 500
          }
        },
        {
          "type": "basic",
          "from": {
            "key_code": "w",
            "modifiers": {
              "mandatory": [

              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open -a finder.app ~/Downloads"
            }
          ],
          "conditions": [
            {
              "type": "variable_if",
              "name": "launcher_mode",
              "value": 1
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "simultaneous": [
              {
                "key_code": "f"
              },
              {
                "key_code": "w"
              }
            ],
            "simultaneous_options": {
              "key_down_order": "strict",
              "key_up_order": "strict_inverse",
              "to_after_key_up": [
                {
                  "set_variable": {
                    "name": "launcher_mode",
                    "value": 0
                  }
                }
              ]
            },
            "modifiers": {
              "mandatory": [

              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "set_variable": {
                "name": "launcher_mode",
                "value": 1
              }
            },
            {
              "shell_command": "open -a finder.app ~/Downloads"
            }
          ],
          "parameters": {
            "basic.simultaneous_threshold_milliseconds": 500
          }
        }
      ]
    }
  ]
}
